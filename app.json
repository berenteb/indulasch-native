{
  "expo": {
    "owner": "berenteb",
    "name": "IndulaSch",
    "slug": "indulasch",
    "scheme": "indulasch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#52155A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "bundleIdentifier": "hu.kirdev.indulasch",
      "buildNumber": "5"
    },
    "android": {
      "package": "hu.kirdev.indulasch",
      "icon": "./assets/images/rounded-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#52155A"
      },
      "versionCode": 4
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Engedélyezd a(z) $(PRODUCT_NAME) számára, hogy a jelenlegi helyzetedet használja!"
        }
      ]
    ]
  }
}
