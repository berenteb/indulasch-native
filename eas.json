{
  "cli": {
    "version": ">= 3.4.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "production": {
      "android": {
        "autoIncrement": true
      },
      "env": {
        "ENVIRONMENT": "production"
      },
      "ios": {
        "resourceClass": "m1-medium",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
