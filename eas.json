{
  "cli": {
    "version": ">= 3.4.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "channel": "preview"
    },
    "production": {
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "resourceClass": "m1-medium",
        "autoIncrement": true
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
